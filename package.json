{
  "name": "shougram-admin",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc",
    "format": "prettier --write \"./**/*.{js,jsx,ts,tsx,md,json}\" --config ./.prettierrc",
    "openapi:download": "curl \"https://staging.api.shougram.uz/doc-json\" -o ./src/api/openapi.json",
    "openapi:generate": "openapi-generator-cli generate -i ./src/api/openapi.json -g typescript-axios --type-mappings=Byte=string --type-mappings=Uri=string --skip-validate-spec --enable-post-process-file -o ./src/api/generated",
    "postinstall": "husky install",
    "openapi": "yarn openapi:download && yarn openapi:generate",
    "commit": "cz"
  },
  "dependencies": {
    "@mux/mux-player-react": "^2.0.1",
    "@nestjsx/crud-request": "^4.1.0",
    "jwt-decode": "^3.1.2",
    "lodash.omitby": "^4.6.0",
    "moment": "^2.29.4",
    "query-string": "^7.1.1",
    "ra-data-nestjsx-crud": "^1.3.2",
    "react": "^18.2.0",
    "react-admin": "^4.15.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.36.1",
    "react-router-dom": "^6.4.1",
    "react-scripts": "5.0.1",
    "recharts": "^2.5.0",
    "styled-components": "^5.3.6",
    "typescript": "^4.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@editorjs/image": "AntonMoldakov/editorjs-image",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@types/lodash.omitby": "^4.6.7",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.27",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitejs/plugin-react": "^3.1.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.2.5",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "install-peers-cli": "^2.2.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prop-types": "^15.8.1",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vite-tsconfig-paths": "^4.0.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
